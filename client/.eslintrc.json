{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "react/prop-types": "off",
        "react/destructuring-assignment": "off",
        "react/jsx-props-no-spreading": "off"
    }
}
